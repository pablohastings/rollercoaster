package  {	import flash.display.MovieClip;	import flash.events.Event;		public class BoundaryObject extends MovieClip {		protected var downwardVelocity:Number; 		protected var isRunning:Boolean;		protected var isJumping:Boolean;		public function BoundaryObject() {						trace ("i am any object that collides with the boundary" );						downwardVelocity = 0;						isRunning = false;						this.gotoAndStop("jump");						addEventListener(Event.ENTER_FRAME, enterFrameHandler );					}				private function enterFrameHandler (event:Event ) :void {						downwardVelocity += 5; // 5 equals itself plus 2			this.y += downwardVelocity;					}				public function incrementUpward() 		{
						//increment the y up until not colliding			this.y -= 0.1;   //.1					}		public function keepOnBoundary() 		{
						downwardVelocity = 0; // stops pulling the object down			positionOnLand();					}				public function positionOnLand()		{			//let this be overridden by the subclass		}			}	}